    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "17.x",
    "react-audio-player": "^0.17.0",
    "react-dom": "17.x",
    "react-svg": "^15.1.17",
    "umi": "^3.5.17"
